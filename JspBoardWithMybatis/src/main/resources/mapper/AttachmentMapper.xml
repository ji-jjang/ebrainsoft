<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.juny.jspboardwithmybatis.domain.board.mapper.AttachmentMapper">
  <select id="findAttachmentById">
    SELECT id,
           logical_name,
           stored_name,
           stored_path,
           extension, size, board_id
    FROM
      attachments
    where
      id = #{id};
  </select>
</mapper>